<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="600.0" prefWidth="1000.0" stylesheets="@css/meucarrinho.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.tecgames.controller.MeucarrinhoController">
   <children>
      <HBox fx:id="divheader" prefHeight="88.0" prefWidth="1000.0">
         <children>
            <Pane prefHeight="88.0" prefWidth="298.0">
               <children>
                  <Button fx:id="voltar" layoutX="14.0" layoutY="26.0" mnemonicParsing="false" onAction="#onVoltarButtonClick" prefHeight="36.0" prefWidth="61.0" stylesheets="@css/voltar.css" />
                  <Label layoutX="82.0" layoutY="31.0" text="Voltar para home" textFill="#2b4c7e">
                     <font>
                        <Font name="System Bold" size="17.0" />
                     </font>
                  </Label>
               </children>
            </Pane>
            <Pane prefHeight="88.0" prefWidth="383.0">
               <children>
                  <Label layoutX="100.0" layoutY="24.0" text="Meu carrinho" textFill="#2b4c7e">
                     <font>
                        <Font name="System Bold" size="24.0" />
                     </font>
                  </Label>
               </children></Pane>
            <Pane prefHeight="200.0" prefWidth="200.0" />
         </children></HBox>
      <AnchorPane fx:id="divcontent" prefHeight="512.0" prefWidth="1000.0" />
   </children>
</VBox>
